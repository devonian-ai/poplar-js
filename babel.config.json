{
    "env": {
      "cjs": {
        "presets": [
          [
            "@babel/preset-env",
            {
              "targets": {
                "node": "current"
              }
            }
          ]
        ],
        "plugins": [
          ["@babel/plugin-transform-modules-commonjs"]
        ]
      },
      "esm": {
        "presets": [
          [
            "@babel/preset-env",
            {
              "targets": {
                "esmodules": true
              }
            }
          ]
        ],
        "plugins": [
          ["@babel/plugin-transform-modules-commonjs", { "loose": true }]
        ]
      }
    }
  }
  